<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="800.0" prefWidth="1300.0" styleClass="a1" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StaffSceneController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
    <children>
        <HBox alignment="TOP_RIGHT" prefHeight="46.0" prefWidth="1300.0" styleClass="a1">
            <children>
                <Button mnemonicParsing="false" onMouseClicked="#close" prefWidth="30.0" styleClass="a2">
                    <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true" styleClass="bb">
                            <image>
                                <Image url="@close.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <HBox.margin>
                        <Insets right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children></HBox>
        <HBox alignment="CENTER" prefHeight="755.0" prefWidth="1300.0" style="-fx-background-color: #0d1026;">
         <children>
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" styleClass="si" text="Add product">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" styleClass="si" text="View Orders">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" styleClass="si" text="Back To Main" />
         </children></HBox>
    </children>
</VBox>
